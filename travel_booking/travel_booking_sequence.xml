<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_type_travel_booking" model="ir.sequence.type">
            <field name="name">Travel Booking</field>
            <field name="code">travel.booking</field>
        </record>

        <record id="seq_travel_booking" model="ir.sequence">
            <field name="name">Travel Booking</field>
            <field name="code">travel.booking</field>
            <field name="padding">3</field>
            <field name="book_no" eval="False"/>
        </record>

        <record id="seq_travel_booking" model="ir.sequence">
            <field name="name">Travel Booking</field>
            <field name="code">travel.booking</field>
            <field name="padding">3</field>
            <field name="customer_id" eval="False"/>
        </record>

        <record id="seq_type_traveller_line" model="ir.sequence.type">
            <field name="name">Traveller Details</field>
            <field name="code">traveller.details</field>
        </record>        

        <record id="seq_traveller_details" model="ir.sequence">
            <field name="name">Traveller Details</field>
            <field name="code">traveller.details</field>
            <field name="padding">3</field>
            <field name="traveller_id" eval="False"/>
        </record>

        <record id="seq_type_travel_line" model="ir.sequence.type">
            <field name="name">Travel Line</field>
            <field name="code">travel.line</field>
        </record>

        <record id="seq_travel_line" model="ir.sequence">
            <field name="name">Travel Line</field>
            <field name="code">travel.line</field>
            <field name="padding">3</field>
            <field name="travel_id" eval="False"/>
        </record>

    </data>
</openerp>